{
  "tag-pair": true,  // Ensure that every opening tag has a corresponding closing tag
  "attr-value-double-quotes": true,  // Ensure that attribute values are enclosed in double quotes
  "doctype-first": true,  // Ensure the doctype is the first thing in the HTML document
  "id-unique": true,  // Ensure that every ID attribute is unique within the document
  "img-alt-require": true,  // Ensure that all <img> tags have an alt attribute
  "title-require": true,  // Ensure that the <title> tag is present and not empty
  "tagname-lowercase": true,  // Ensure that all tag names are in lowercase
  "attr-lowercase": true,  // Ensure that all attribute names are in lowercase
  "attr-no-duplication": true,  // Ensure that no duplicate attributes exist on an element
  "spec-char-escape": true,  // Ensure that special characters are escaped properly
  "head-script-disabled": true,  // Prevent the use of <script> tags in <head>
  "style-disabled": true,  // Prevent the use of <style> tags in the HTML document
  "html-req-lang": true,  // Ensure that the <html> tag has a lang attribute
  "html-req-closure": true,  // Ensure that the <html> tag is properly closed
  "attr-no-duplication": true,  // Ensure that no duplicate attributes exist on an element
  "inline-style-disabled": true,  // Prevent the use of inline styles
  "attr-value-not-empty": true,  // Ensure that attribute values are not empty
  "element-no-unknown": true,  // Ensure that only known HTML elements are used
  "tag-self-close": true,  // Ensure that void elements are self-closing
  "head-valid-content-model": true,  // Ensure valid content within the <head> element
  "body-valid-content-model": true,  // Ensure valid content within the <body> element
  "no-mixed-content": true  // Ensure that there is no mixed content (HTTP and HTTPS) in the document
}
